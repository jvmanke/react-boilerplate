{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "A react boilerplate",
  "main": "src/index.js",
  "scripts": {
    "dev": "node devServe.js",
    "build": "webpack --mode production",
    "lint": "npm run lint:eslint && npm run lint:stylelint",
    "lint:eslint": "eslint src *.js",
    "lint:stylelint": "stylelint src/**/*.js",
    "component": "bash ./scripts/react-component.sh src/components",
    "view": "bash ./scripts/react-component.sh src/views",
    "reducer": "bash ./scripts/redux-reducer.sh src/modules"
  },
  "keywords": [
    "React",
    "Config"
  ],
  "author": "Jo√£o Victor Manke",
  "license": "MIT",
  "dependencies": {
    "express": "^4.16.4",
    "normalize.css": "^8.0.1",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-redux": "^6.0.1",
    "redux": "^4.0.1",
    "redux-define": "^1.1.1",
    "redux-thunk": "^2.3.0",
    "wretch": "^1.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.1",
    "eslint": "^5.15.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.5.0",
    "style-loader": "^0.23.1",
    "stylelint": "^9.10.1",
    "stylelint-config-standard": "^18.2.0",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-dev-middleware": "^3.6.1",
    "webpack-hot-middleware": "^2.24.3"
  }
}
